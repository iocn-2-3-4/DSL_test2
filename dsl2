pipeline {
    agent none
    stages {
                stage('test') {
             agent {
                 label 'master'
             }
             steps {   
        readFileFromWorkspace('test2')
    	  }
            }
        }
     stage('build') {
             agent {
                 label 'master'
             }
             steps {   
                checkout scm
                sh 'echo from master'
            }
   
}
}
