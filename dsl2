#!groovy
def releaseScript = readFileFromWorkspace('test2')
pipeline {
    agent none
    stages {
                stage('test') {
             agent {
                 label 'master'
             }
             steps {   
        groovyCommand(releaseScript)
    	  }
            }
        }
     stage('build') {
             agent {
                 label 'master'
             }
             steps {   
                checkout scm
                sh 'echo from master'
            }
   
}
