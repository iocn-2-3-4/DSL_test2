#!groovy
import groovy.io.FileType
import hudson.FilePath
import hudson.*


job("project_test/test") {
    logRotator(-1, 10)

    triggers {
        // githubPush()
        scm('H/2 * * * *')
    }
    steps{
        load './test2'

    }

}
job("project_test/build") {
    logRotator(-1, 10)

    triggers {
        // githubPush()
        scm('H/2 * * * *')
    }
    steps{
        shell "echo 'buils code'"
    }

}
job("project_test/deploy") {
    logRotator(-1, 10)

    triggers {
        // githubPush()
        scm('H/2 * * * *')
    }
    steps{
        load 'deploy2'
    }

}
